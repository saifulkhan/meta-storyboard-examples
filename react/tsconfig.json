{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "jsxImportSource": "@emotion/react",
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ]
    },
    "typeRoots": [
      "src/types",
      "node_modules/@types"
    ],
    "noFallthroughCasesInSwitch": true,
    "incremental": true
  },
  "include": [
    "src",
    "assets"
  ],
  "exclude": [
    "node_modules"
  ]
}